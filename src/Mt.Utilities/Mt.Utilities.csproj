<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>net8.0</TargetFrameworks>
    <IsPackable>true</IsPackable>
    <PackageId>Mt.Utilities</PackageId>
    <PackageTags>Mt;Net;Utilities;</PackageTags>
    <Description>Универсальные утилиты используемые в проектах Mt Relay.</Description>
    <AssemblyOriginatorKeyFile>$(MSBuildThisFileDirectory)mt-utilities.snk</AssemblyOriginatorKeyFile>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
  </PropertyGroup>
  <ItemGroup>
    <None Include="..\..\README.md" Pack="true" PackagePath="\" />
    <None Include="..\..\iconMt.png" Pack="true" PackagePath="\" />
  </ItemGroup>
</Project>